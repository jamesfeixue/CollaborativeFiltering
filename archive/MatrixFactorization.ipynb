    "#create sparse train matrix games by customers with ratings - hours\n",
    "M=np.zeros((len(items),len(users)))\n",
    "\n",
    "for index, row in trainRatings.iterrows():\n",
    "    M[items.index(row['gameID'])][users.index(row['userIndex'])]=row['hours']\n",
    "\n",
    "M = csr_matrix(M)\n",
    "\n",
    "print M.shape"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 143,
   "metadata": {},
